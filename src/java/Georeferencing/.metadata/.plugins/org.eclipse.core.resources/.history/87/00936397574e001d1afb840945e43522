package ca.cidco.csb.ppp;

import java.util.Vector;

public class PppPosition {
	String date;
	Double year;
	Double month;
	Double day;
	String time;
	Double hr;
	Double mn;
	Double ss;	
	Double nsv;
	Double gdop;
	Double sdlat;
	Double sdlon;
	Double sdhgt;
//	Vector<Double> position;
	
	
	
	public PppPosition(String in_date, String in_time, String in_nsv, String in_gdop, String in_sdlat, String in_sdlon, String in_sdhgt) {
//		position= new Vector<>();
		date = in_date;
		time = in_time;
		nsv = Double.valueOf(in_nsv);
		gdop = Double.valueOf(in_gdop);
		sdlat = Double.valueOf(in_sdlat);
		sdlon = Double.valueOf(in_sdlon);
		sdhgt = Double.valueOf(in_sdhgt);
		String[] splitDate= in_date.split("[-]{1,}");
		year = Double.valueOf(splitDate[0]);
		month = Double.valueOf(splitDate[1]);
		day = Double.valueOf(splitDate[2]);
		String[] splitTime= in_time.split("[\\:]{1,}");
		hr = Double.valueOf(splitTime[0]);
		mn = Double.valueOf(splitTime[1]);
		ss = Double.valueOf(splitTime[2]);
//		position.add(year);
//		position.add(month);		
//		position.add(day);		
//		position.add(hr);
//		position.add(mn);
//		position.add(ss);
//		position.add(nsv);
//		position.add(gdop);
//		position.add(sdlat);
//		position.add(sdlon);
//		position.add(sdhgt);
		
	}





	public String getDate() {
		return date;
	}


	public Double getYear() {
		return year;
	}


	public Double getMonth() {
		return month;
	}


	public Double getDay() {
		return day;
	}


	public String getTime() {
		return time;
	}


	public Double getHr() {
		return hr;
	}


	public Double getMn() {
		return mn;
	}


	public Double getSs() {
		return ss;
	}


	public Double getNsv() {
		return nsv;
	}


	public Double getGdop() {
		return gdop;
	}


	public Double getSdlat() {
		return sdlat;
	}


	public Double getSdlon() {
		return sdlon;
	}


	public Double getSdhgt() {
		return sdhgt;
	}


    // Overriding toString() method of String class
    @Override
    public String toString() {
        return "Date : "+ date+" | Time : "+time +" | NVS : "+ nsv+" | GDOP : "+gdop + " | SDLAT : "+sdlat+" | SDLON : "+sdlon+" | SDHGT : "+sdhgt +"\n";
    }
}